{
  "extends": "./tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": false,
    "importsNotUsedAsValues": "remove"
  },
  "exclude": ["build", "test/fixtures/**/*.*", "template/**/*.*"]
}
